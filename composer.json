{
    "name": "ongr/oxid-connector-bundle",
    "description": "OXID connector for ONGR framework",
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.nfq.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:ongr-io/ConnectionsBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:ongr-io/TestingBundle.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ongr/connections-bundle": "dev-master",
        "ongr/elasticsearch-bundle": "~0.1",
        "ongr/content-bundle": "~0.1",
        "ongr/router-bundle": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "ongr/testing-bundle": "dev-master",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "suggest": {
        "ongr/content-bundle": "Allows to transfer CMS data from OXID to ONGR",
        "ongr/router-bundle": "Adds SEO friendly URLs"
    },
    "autoload": {
        "psr-4": {
            "ONGR\\OXIDConnectorBundle\\": ""
        }
    },
    "target-dir": "ONGR/OXIDConnectorBundle"
}
